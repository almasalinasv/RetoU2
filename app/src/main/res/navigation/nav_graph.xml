<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/formFragment">

    <fragment
        android:id="@+id/formFragment"
        android:name="com.example.retou2.FormFragment"
        android:label="fragment_form"
        tools:layout="@layout/fragment_form" >
        <action
            android:id="@+id/action_formFragment_to_datosFragment"
            app:destination="@id/datosFragment" />
    </fragment>
    <fragment
        android:id="@+id/datosFragment"
        android:name="com.example.retou2.datosFragment"
        android:label="fragment_datos"
        tools:layout="@layout/fragment_datos" >
        <argument
            android:name="argumentoPasado"
            app:argType="string" />
        <argument
            android:name="argumentoPasado2"
            app:argType="string" />
        <argument
            android:name="argumentoPasado3"
            app:argType="string" />
        <argument
            android:name="argumentoPasado4"
            app:argType="string" />
        <argument
            android:name="argumentoPasado5"
            app:argType="string" />
    </fragment>
</navigation>